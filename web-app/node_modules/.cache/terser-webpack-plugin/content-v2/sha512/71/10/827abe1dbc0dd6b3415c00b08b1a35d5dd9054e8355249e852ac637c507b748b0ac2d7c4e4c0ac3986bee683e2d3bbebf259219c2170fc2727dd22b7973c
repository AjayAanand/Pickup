ledby=\"confirmation-dialog-title\"\r\n      open={open}\r\n      {...other}\r\n      onClose={(event, reason) => {\r\n        if (reason !== 'backdropClick') {\r\n          onClose()\r\n        }\r\n      }}\r\n      style={{direction:isRTL==='rtl'? 'rtl':'ltr'}}\r\n    >\r\n      <DialogTitle id=\"confirmation-dialog-title\" style={{textAlign:'center'}}>{t('select_reason')}</DialogTitle>\r\n      <DialogContent dividers>\r\n        <RadioGroup\r\n          ref={radioGroupRef}\r\n          aria-label=\"ringtone\"\r\n          name=\"ringtone\"\r\n          value={value}\r\n          onChange={handleChange}\r\n        >\r\n          {options.map((option) => (\r\n            <FormControlLabel value={option} key={option} control={<Radio style={{margin:10}}/>} label={option} style={{padding:8}}/>\r\n          ))}\r\n        </RadioGroup>\r\n      </DialogContent>\r\n      <DialogActions>\r\n        <Button autoFocus onClick={handleCancel} color=\"primary\">\r\n          {t('cancel')}\r\n        </Button>\r\n        <Button onClick={handleOk} color=\"primary\">\r\n          {t('ok')}\r\n        </Button>\r\n      </DialogActions>\r\n    </Dialog>\r\n  );\r\n}\r\n\r\nConfirmationDialogRaw.propTypes = {\r\n  onClose: PropTypes.func.isRequired,\r\n  open: PropTypes.bool.isRequired,\r\n};","/* eslint-disable no-use-before-define */\r\nimport React from 'react';\r\nimport TextField from '@mui/material/TextField';\r\nimport { Autocomplete } from '@mui/material';\r\nimport { makeStyles } from '@mui/styles';\r\nimport { useTranslation } from \"react-i18next\";\r\n\r\nconst useStyles = makeStyles({\r\n  option1: {\r\n    fontSize: 15,\r\n    '& > span': {\r\n      marginRight: 10,\r\n      fontSize: 18,\r\n    }\r\n  },\r\n  option2: {\r\n    fontSize: 15,\r\n    '& > span': {\r\n      marginRight: 10,\r\n      fontSize: 18,\r\n    },\r\n    direction:'rtl'\r\n  },\r\n  inputRtl: {\r\n    \"& label\": {\r\n      right: 75,\r\n      left: \"auto\"\r\n    },\r\n    \"& legend\": {